{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "singularName": "booking",
    "pluralName": "bookings",
    "displayName": "Booking",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "note": {
      "type": "text"
    },
    "numberPeople": {
      "type": "integer"
    },
    "dateStart": {
      "type": "date"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Approve",
        "Cancle"
      ],
      "default": "Pending",
      "required": true
    },
    "statusPayment": {
      "type": "enumeration",
      "enum": [
        "Yes",
        "No"
      ],
      "required": true,
      "default": "No"
    },
    "tour": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tour.tour"
    },
    "total": {
      "type": "string"
    }
  }
}
